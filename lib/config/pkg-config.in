prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=@PKG_CONFIG_EXEC_PREFIX@
libdir=${prefix}/@PKG_CONFIG_LIBDIR@
includedir=${prefix}/@PKG_CONFIG_INCLUDEDIR@

Name: @PKG_CONFIG_LIBRARY_NAME@
Description: @CMAKE_PROJECT_DESCRIPTION@
URL: @CMAKE_PROJECT_HOMEPAGE_URL@
Version: @PROJECT_VERSION@
Requires:@PKG_CONFIG_REQUIRES@
Requires.private:
Cflags: @PKG_CONFIG_INCLUDE_PREFIX@${includedir}@PKG_CONFIG_CFLAGS@
Libs: @PKG_CONFIG_LIBPATH_PREFIX@${libdir} @PKG_CONFIG_LIB_PREFIX@@PKG_CONFIG_LIBRARY_NAME@@PKG_CONFIG_LIB_SUFFIX@@PKG_CONFIG_LIBS@
Libs.private:
